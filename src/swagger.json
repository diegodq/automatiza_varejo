				"tags": ["TypeCustomer"],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/TypeCustomer"
							},
							"examples": {
								"TypeCustomer": {
									"value": {
										"type_customer": "GERENTE"
									}
								}
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "new-customer-added"
					},
					"400": {
						"description": "type-customer-already-exists"
